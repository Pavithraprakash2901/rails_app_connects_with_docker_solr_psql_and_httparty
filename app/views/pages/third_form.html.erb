
<div class="container">

  <h1 class="text-center text-primary">Official Information Form</h1>

 

  <%= form_with(model: @third_form, url: third_form_path, method: :post, local: true) do |form| %>

    <div class="form-group mb-3">

      <%= form.label :name, "Name" %>

      <%= form.text_field :name, class: "form-control",required: true, placeholder: "Enter your name" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :email, "Email" %>

      <%= form.email_field :email, class: "form-control",required: true, placeholder: "Enter your email" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :phone_no, "Phone No" %>

      <%= form.text_field :phone_no, class: "form-control",required: true, placeholder: "Enter your phone number" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :tenth_mark, "10th Mark" %>

      <%= form.text_field :tenth_mark, class: "form-control",required: true, placeholder: "Enter your 10th mark" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :twelfth_mark, "12th Mark" %>

      <%= form.text_field :twelfth_mark, class: "form-control",required: true, placeholder: "Enter your 12th mark" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :cgpa, "CGPA" %>

      <%= form.text_field :cgpa, class: "form-control",required: true, placeholder: "Enter your CGPA" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :aadhar_no, "Aadhar No" %>

      <%= form.text_field :aadhar_no, class: "form-control",required: true, placeholder: "Enter your Aadhar number" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :pan_no, "PAN No" %>

      <%= form.text_field :pan_no, class: "form-control",required: true, placeholder: "Enter your PAN number" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :account_no, "Account No" %>

      <%= form.text_field :account_no, class: "form-control",required: true, placeholder: "Enter your account number" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :no_of_siblings, "No of Siblings" %>

      <%= form.number_field :no_of_siblings, class: "form-control", min: 0,required: true, placeholder: "Enter the number of siblings" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :first_graduate, "First Graduate?" %>

      <%= form.select :first_graduate, options_for_select(["Yes", "No"]), class: "form-control" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :skills, "Skills" %>

      <%= form.text_field :skills, class: "form-control",required: true, placeholder: "Enter your skills" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :differently_abled, "Differently Abled?" %>

      <%= form.select :differently_abled, options_for_select(["Yes", "No"]), class: "form-control" %>

    </div>

 

    <div class="form-group mb-3">

      <%= form.label :completed_graduation, "Completed Graduation?" %>

      <%= form.select :completed_graduation, options_for_select(["Yes", "No"]), class: "form-control" %>

    </div>

 

    <%= form.submit "Submit", class: "btn btn-primary" %>

    <%= form.button "Clear", type: "reset", class: "btn btn-danger" %>

  <% end %>

</div>

 